{
    "installCommand": "npm install --legacy-peer-deps",
    "builds":[
        {
            "src": "src/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
        
    ]
}